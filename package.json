  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "tsc --project tsconfig.json",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\"",
